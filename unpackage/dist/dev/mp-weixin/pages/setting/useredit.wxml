<view><cu-custom vue-id="0bd66a0e-1" bgColor="bg-gradual-pink" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">编辑资料</view></cu-custom><form><view class="cu-form-group"><view class="title">姓名</view><input placeholder="请输入姓名" name="input" data-event-opts="{{[['input',[['__set_model',['$0','realname','$event',[]],['myFormData']]]]]}}" value="{{myFormData.realname}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">用户名</view><input placeholder="用户名" name="input" disabled="{{true}}" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['myFormData']]]]]}}" value="{{myFormData.username}}" bindinput="__e"/></view><view class="cu-form-group"><view class="title">头像</view><view class="grid col-4 grid-square flex-sub"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-img" data-url="{{imgList[index]}}" data-event-opts="{{[['tap',[['ViewImage',['$event']]]]]}}" bindtap="__e"><image src="{{imgList[index]}}" mode="aspectFill"></image><view class="cu-tag bg-red radius" data-index="{{index}}" data-event-opts="{{[['tap',[['DelImg',['$event']]]]]}}" catchtap="__e"><text class="cuIcon-close"></text></view></view></block><block wx:if="{{imgList.length<1}}"><view data-event-opts="{{[['tap',[['ChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></block></view></view><view class="cu-form-group margin-top"><view class="title">性别</view><switch class="{{['switch-sex',switchC?'checked':'']}}" checked="{{switchC?true:false}}" data-event-opts="{{[['change',[['SwitchC',['$event']]]]]}}" bindchange="__e"></switch></view><view class="cu-form-group margin-top"><view class="title">手机号码</view><input placeholder="输入手机号码" name="input" data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['myFormData']]]]]}}" value="{{myFormData.phone}}" bindinput="__e"/><view class=